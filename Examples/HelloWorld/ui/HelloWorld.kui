element HelloWorld
{
	size = 2sr;
	position = -1;

	horizontalAlign = centered;
	verticalAlign = centered;

	orientation = vertical;

	child UIText
	{
		text = "Hello World!";
		size = 18px;
	}
	child UITextField
	{
		text = "Some editable text";
		width = 200px;
		textSize = 13px;
		allowNewLine = true;
		textColor = 0;
	}
}